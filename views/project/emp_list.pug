extends ../layout/emp_layout.pug

block content
  h1= title
  p= user

  .card.mb-3
    .card-header
      i.fa.fa-table
      |  project list
    .card-body
      .table-responsive
        table#dataTable.table.table-bordered(width='100%', cellspacing='0')
          thead
            tr
              th 프로젝트 id
              th 프로젝트 명
              th 시작일
              th 종료일
            tbody
              each pro in projects
                tr
                  td= pro.project_id
                  td= pro.name
                  td= pro.start_date
                  td= pro.end_date


  
